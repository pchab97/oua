<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.applecatalog">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".InfoActivity"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".TabletsActivity"
            android:parentActivityName=".TabletsCategoryActivity"/>
        <activity android:name=".TabletsCategoryActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ComputerActivity"
            android:parentActivityName=".ComputerCategoryActivity"/>
        <activity android:name=".ComputerCategoryActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".PhoneeActivity"
            android:parentActivityName=".PhoneActivity"/>
        <activity
            android:name=".PhoneActivity"
            android:label="@string/titleCategory" android:parentActivityName=".MainActivity"
            />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>