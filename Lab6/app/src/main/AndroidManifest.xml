<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lab6">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.lab6.Activities.OrderActivity"
            android:label="@string/orders_text"
            android:parentActivityName="com.example.lab6.Activities.MainActivity" />
        <activity
            android:name="com.example.lab6.Activities.SubmitActivity"
            android:label="@string/submitActivity_name"
            android:parentActivityName="com.example.lab6.Activities.MainActivity" />
        <activity
            android:name="com.example.lab6.Activities.InfoActivity"
            android:label="@string/infoActivity_name"
            android:parentActivityName="com.example.lab6.Activities.MainActivity" />
        <activity
            android:name="com.example.lab6.Activities.CategoryActivity"
            android:label="@string/categoryActivity_name"
            android:parentActivityName="com.example.lab6.Activities.MainActivity" />
        <activity
            android:name="com.example.lab6.Activities.DetailsActivity"
            android:label="@string/detailsActivity_name"
            android:parentActivityName="com.example.lab6.Activities.MainActivity" />
        <activity
            android:name="com.example.lab6.Activities.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>